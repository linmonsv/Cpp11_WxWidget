<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="wxButtonTextFrame">
		<size>330,121</size>
		<id_arg>0</id_arg>
		<object class="wxGridSizer" variable="GridSizer1" member="no">
